{
  "name": "membergate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "josh kennedy <josh@outthinkgroup.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "yarn env:dev && export APP_ENV=development && vite",
    "env:dev": "sed -i '' 's/production/development/g' env.php;",
    "build": "yarn env:build && vite build",
    "env:build": "sed -i '' 's/development/production/g' env.php && sed -i '' 's/test/production/g' env.php",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": " sed -i '' 's/production/development/g' env.php && cypress open",
    "start-test": "node ./../esp-mock-server/ . >./../esp-mock-server/logs.log 2>&1 &",
    "end-test": "../esp-mock-server/req/kill",
    "test-all": "npx cypress run --record --key bd3d921a-f7d2-4711-9174-798eee03d9da",
		"block:dev":"wp-scripts start --webpack-src-dir=extend-block-editor/src --output-path=extend-block-editor/build",
		"block:build":"wp-scripts build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.9",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.11.0",
    "postcss": "^8.4.19",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.1",
    "@types/chai": "^4.3.5",
    "@wordpress/scripts": "^26.10.0",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "lucide-svelte": "^0.258.0",
    "radix-svelte": "^0.8.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6"
  }
}
